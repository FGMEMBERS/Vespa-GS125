<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oilpressure.ac</path>

    <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
     <object-name>vitre</object-name>
  </effect>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>     <type>shader</type>     <shader>chrome</shader>     <texture>glass.png</texture>     <object-name>vitre</object-name>
  </animation>    <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.8 </red>
      <green> 0.7 </green>
      <blue>  0.5 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine/oilp-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   0 </dep></entry>
      <entry><ind> 0.2 </ind><dep>  112 </dep></entry>
      <entry><ind> 0.4 </ind><dep> 126 </dep></entry>
      <entry><ind> 0.6 </ind><dep> 131 </dep></entry>
      <entry><ind> 0.8 </ind><dep> 137 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 142 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


