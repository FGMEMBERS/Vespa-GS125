<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>rpm.ac</path>

    <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
     <object-name>vitre</object-name>
  </effect>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>needle2</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>needle2</object-name>
    <emission>
      <red>   0.03125 </red>
      <green> 0.00890 </green>
      <blue>  0.00890 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>alpha-test</type>
    <alpha-factor> 0.05 </alpha-factor>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/rpm</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  750 </ind><dep>  20.5 </dep></entry>
      <entry><ind>  1500 </ind><dep>  41 </dep></entry>
      <entry><ind>  2250 </ind><dep>  61.5 </dep></entry>
      <entry><ind>  3000 </ind><dep> 82 </dep></entry>
      <entry><ind>  3750 </ind><dep> 102.5 </dep></entry>
      <entry><ind>  4500 </ind><dep> 123 </dep></entry>
      <entry><ind>  5250 </ind><dep> 143.5 </dep></entry>
      <entry><ind>  6000 </ind><dep> 164 </dep></entry>
      <entry><ind>  6750 </ind><dep> 184.5 </dep></entry>
      <entry><ind>  7500 </ind><dep> 205 </dep></entry>
      <entry><ind> 8250 </ind><dep> 225.5 </dep></entry>
      <entry><ind> 9000 </ind><dep> 246 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle2</object-name>
    <property>engines/engine/epr</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 2.994 </ind><dep> 3600 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.009 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
